#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/internnova/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/bin/node_modules:/workspace/internnova/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/node_modules:/workspace/internnova/node_modules/.pnpm/typescript@4.5.4/node_modules:/workspace/internnova/node_modules/.pnpm/node_modules:/workspace/internnova/node_modules:/workspace/node_modules:/node_modules:/workspace/internnova/apps/server/node_modules:/workspace/internnova/apps/node_modules"
else
  export NODE_PATH="$NODE_PATH:/workspace/internnova/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/bin/node_modules:/workspace/internnova/node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/node_modules:/workspace/internnova/node_modules/.pnpm/typescript@4.5.4/node_modules:/workspace/internnova/node_modules/.pnpm/node_modules:/workspace/internnova/node_modules:/workspace/node_modules:/node_modules:/workspace/internnova/apps/server/node_modules:/workspace/internnova/apps/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/typescript@4.5.4/node_modules/typescript/bin/tsc" "$@"
fi
